  "scripts": {
    "build": "cross-env NODE_ENV='production' webpack --config config/webpack.prod.config.js --mode production",
    "dev": "cross-env NODE_ENV='development' webpack-dev-server --open --config config/webpack.dev.config.js --mode development",
    "dll": "webpack --config config/webpack.dll.config.js --mode production",
    "start": "npm run dll && npm run dev",
    "prod": "npm run dll && npm run build",
    "server": "node node_server/panoDataToJson.js && npm run prod"
