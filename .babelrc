{
  "presets": ["react", "env","es2015","stage-0"],
  "env": {
    "development": {
      "presets": ["react-hmre"],
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",

            "imports": ["react"],

            "locals": ["module"]
          }]
        }]
      ]
    }
  }
}